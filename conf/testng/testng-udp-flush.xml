<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="udp-flush"
       parallel="tests"
       thread-count="1"
        >


    <test name="udp-flush" junit="false">
        <parameter name="channel.conf" value="flush-udp.xml"/>
        <groups>
            <run>
                <include name="stack-dependent"/>
                <include name="flush"/>
                <exclude name="vlad"/>
            </run>
        </groups>
        <packages>
            <package name="org.jgroups.tests.*"/>
            <package name="org.jgroups.blocks.*"/>
            <package name="org.jgroups.protocols.*"/>
        </packages>
    </test>

</suite>
