<!--
Describes the version(s) of the libraries JBossCache depends on
Author: Bela Ban
Version: $Id: dependencies.xml,v 1.1 2005/05/30 10:46:54 belaban Exp $
 -->

<dependencies>
    <jdk>1.4</jdk>
    <jdk>1.5</jdk>

    <lib name="ant-optional">
        <jar>ant-optional.jar</jar>
        <version>1.5.2</version>
        <description>Optional tasks for ANT</description>
    </lib>

    <lib name="ant">
        <jar>ant.jar</jar>
        <version>1.5.2</version>
        <description>ANT</description>
    </lib>

    <lib name="avalon">
        <jar>avalon-framework-4.0.jar</jar>
        <version>4.0</version>
        <description>Used to generate HTML testreports and PDF documentation</description>
    </lib>

    <lib name="batik">
        <jar>batik.jar</jar>
        <version>n/a</version>
        <description>Used to generate HTML testreports and PDF documentation</description>
    </lib>

    <lib name="bcprov-jdk14-117">
        <jar>bcprov-jdk14-117.jar</jar>
        <version>1.17.0</version>
        <description>Security provider, used by ENCRYPT (www.bouncycastle.org)</description>
    </lib>

    <lib name="beanshell">
        <jar>bsh-1.2b3</jar>
        <version>1.2b3</version>
        <description>Embeddable Java source interpreter. Used by BSH protocol</description>
    </lib>

    <lib name="commons-logging">
        <jar>commons-logging.jar</jar>
        <version>1.0.3</version>
        <description>Apache commons logging. Used in logging now.</description>
    </lib>

    <lib name="concurrent">
        <jar>concurrent.jar</jar>
        <version>1.3.4</version>
        <description>Doug Lea's concurrent package</description>
    </lib>

    <lib name="FOP">
        <jar>fop.jar</jar>
        <version>0.20.3</version>
        <description>Used to generate PDF from XML (docbook)</description>
    </lib>

    <lib name="JMS">
        <jar>jms.jar</jar>
        <version>n/a</version>
        <description>Used by the JMS protocol</description>
    </lib>

    <lib name="junit">
        <jar>junit.jar</jar>
        <version>3.8.1</version>
        <description>Unit testing framework. Used for examples.</description>
    </lib>

    <lib name="log4j">
        <jar>log4j.jar</jar>
        <version>1.2.6</version>
        <description>Logging library</description>
    </lib>

    <lib name="logkit">
        <jar>logkit-1.0b4.jar</jar>
        <version>1.0b4</version>
        <description></description>
    </lib>

    <lib name="xalan">
        <jar>xalan.jar</jar>
        <version>2.4.D1</version>
        <description></description>
    </lib>

</dependencies>





